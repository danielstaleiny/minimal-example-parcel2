{
  "name": "jamstack",
  "version": "1.0.0",
  "description": "Boilerplate for JAMstack website.",
  "scripts": {
    "build": "parcel build '.tmp/parcel/**/index.html' --cache-dir .tmp/.cache",
    "build:dev": "NODE_ENV=development parcel build '.tmp/parcel/**/*.html' --cache-dir .tmp/.cache",
    "watch": "parcel watch '.tmp/parcel/**/*.html' --cache-dir .tmp/.cache",
    "watch:dev": "NODE_ENV=development parcel watch '.tmp/parcel/**/*.html' --cache-dir .tmp/.cache",
    "serve": "parcel serve --port 3000 '.tmp/parcel/**/*.html' --cache-dir .tmp/.cache",
    "serve:dev": "NODE_ENV=development parcel serve --port 3000 '.tmp/parcel/**/*.html' --cache-dir .tmp/.cache",
    "api": "serve -l 3000 dist"
  },
  "alias": {
    "pages": "./pages",
    "includes": "./src/includes",
    "data": "./src/data",
    "css": "./pages/assets/css",
    "js": "./pages/assets/js"
  },
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.10.0",
    "@babel/plugin-transform-strict-mode": "^7.8.3",
    "@babel/polyfill": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "autoprefixer": "^9.7.6",
    "fast-glob": "^3.2.2",
    "npm-check-updates": "^4.1.2",
    "npm-run-all": "^4.1.5",
    "nunjucks": "^3.2.1",
    "parcel": "^2.0.0-nightly.240",
    "postcss-easy-import": "^3.0.0",
    "rimraf": "^3.0.2",
    "serve": "^11.3.0",
    "tailwindcss": "^1.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danielstaleiny/JAMstack.git"
  },
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/danielstaleiny/JAMstack/issues"
  },
  "homepage": "https://github.com/danielstaleiny/JAMstack#readme"
}
